# Cali X One Environment Configuration
# Copy this to .env and fill in your values

# Server Configuration
HOST=0.0.0.0
PORT=8003

# Worker Registry
WORKER_REGISTRY=http://localhost:8003/api/workers

# Worker Ports
JOSEPHINE_PORT=9998
BUBBLE_PORT=9997

# Extension Origins (for CORS)
EXTENSION_ORIGIN=chrome-extension://*

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-here

# Database
DATABASE_URL=sqlite:///./caleon.db

# Vault Configuration
VAULT_ENCRYPTION_KEY=your-256-bit-key-here

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Logging
LOG_LEVEL=INFO